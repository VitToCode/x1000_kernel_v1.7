choice
	prompt "PRODUCT type"
	depends on SOC_M200
	default PRODUCT_DEVELOP

config BOARD_4785_FPGA
       bool "FPGA"

config PRODUCT_DEVELOP
       bool "DEVELOP"

config PRODUCT_RVMS
       bool "RVMS"
endchoice

config PRODUCT_NAME
	string
	default "fpga" if BOARD_4785_FPGA
	default "dorado" if PRODUCT_DEVELOP
	default "rvms" if PRODUCT_RVMS

if PRODUCT_RVMS
source "arch/mips/xburst/soc-m200/board/rvms/Kconfig"
endif

if PRODUCT_DEVELOP
source "arch/mips/xburst/soc-m200/board/dorado/Kconfig"
endif

